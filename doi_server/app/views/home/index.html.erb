<div id="home_index_query">
  <h1>DOI-to-URL Lookup Service</h1>
  <br><br>
  <%= form_tag(query_path method: "get") do %>
    <%= label_tag(:query, "DOI: ") %>
    <%= text_field_tag(:query) %>
    <%= submit_tag("Lookup") %>
  <% end %>
  <br>
</div>
<div id="home_index_new">
  <%= link_to 'Create a new DOI', new_doi_path %>
</div>
